{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "pnpm: dev",
      "command": "cd apps/web && pnpm dev",
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "pattern": {
          "regexp": "^$"
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": "ready in.*"
        }
      },
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    }
  ]
}
